projection;
strict ( 2 );
use side effects;
use draft;

define behavior for ZC_PATIENTS alias Patients
{
  use create;
  use update;
  use delete;

  use action Activate;
  use action Discard;
  use action Edit;
  use action Resume;
  use action Prepare;

  use association _MedicalRecords{create; with draft;}


}
define behavior for ZC_MEDICAL_RECORDS alias MedicalRecords
use etag
{
    use delete;
    use update;
    use association _Patient{with draft;}
}